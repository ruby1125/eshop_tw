<?xml version="1.0"?>
<layout version="0.1.0">
	<miofaq_faqs_index>
		<reference name="head">
			<action method="addItem"><type>skin_js</type><name>js/bootstrap.js</name></action>
			<action method="addItem"><type>skin_css</type><name>css/bootstrap.css</name></action>
		</reference>
		<reference name="root">
			<action method="setTemplate"><template>page/1column.phtml</template></action>
		</reference>
		<reference name="content">
			<block type="miofaq/faqs" name="mio.faqs" template="mio/faq/faqs.phtml"/>
		</reference>
	</miofaq_faqs_index>
</layout>